{
  "name": "react-web-starter",
  "version": "0.1.0",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && pretty-quick --staged"
    }
  },
  "lint-staged": {
    "./*/{js,tsx}": [
      "yarn run prettify"
    ]
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "postinstall": "husky install",
    "prettify": "prettier ./**/*.{js,tsx} --write"
  },
  "dependencies": {
    "@types/node": "^14.14.19",
    "@types/react": "^17.0.0",
    "lint-staged": "^10.5.3",
    "next": "10.0.4",
    "prop-types": "^15.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "sass": "^1.32.0",
    "tachyons": "^4.12.0",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "husky": "^5.0.6",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0"
  }
}
