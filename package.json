{
  "name": "react-web-starter",
  "version": "0.1.0",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "lint-staged": {
    "*.{tsx,ts}": "eslint --fix",
    "*.{tsx, ts}": "prettier --write"
  },
  "settings": {
    "import/resolver": {
      "typescript": {}
    }
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "prettify": "prettier ./**/*.{js,ts,tsx} --write",
    "lint": "pretty-quick --staged && lint-staged"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.5.0",
    "@types/axios": "^0.14.0",
    "@types/humps": "^2.0.0",
    "@types/node": "^14.14.19",
    "@types/normalizr": "^2.0.18",
    "@types/react": "^17.0.0",
    "@types/react-redux": "^7.1.15",
    "@types/redux-logger": "^3.0.8",
    "@types/redux-persist": "^4.3.1",
    "@typescript-eslint/eslint-plugin": "4.0.1",
    "@typescript-eslint/parser": "4.0.1",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-less": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "antd": "^4.10.0",
    "axios": "^0.21.1",
    "babel-plugin-import": "^1.13.3",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4",
    "humps": "^2.0.1",
    "less": "^4.0.0",
    "lint-staged": ">=10",
    "next": "10.0.4",
    "normalizr": "^3.6.1",
    "prop-types": "^15.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "sass": "^1.32.0",
    "tachyons": "^4.12.0",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "eslint": "^7.2.0",
    "husky": "^5.0.6",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "redux-devtools": "^3.7.0"
  }
}
