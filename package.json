{
  "name": "react-web-starter",
  "version": "0.1.0",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && pretty-quick --staged"
    }
  },
  "lint-staged": {
    "./*/{js,ts,tsx}": [
      "yarn run prettify"
    ]
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "postinstall": "husky install",
    "prettify": "prettier ./**/*.{js,ts,tsx} --write"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.5.0",
    "@types/axios": "^0.14.0",
    "@types/humps": "^2.0.0",
    "@types/node": "^14.14.19",
    "@types/normalizr": "^2.0.18",
    "@types/react": "^17.0.0",
    "@types/react-redux": "^7.1.15",
    "@types/redux-logger": "^3.0.8",
    "@types/redux-persist": "^4.3.1",
    "axios": "^0.21.1",
    "humps": "^2.0.1",
    "lint-staged": "^10.5.3",
    "next": "10.0.4",
    "normalizr": "^3.6.1",
    "prop-types": "^15.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "sass": "^1.32.0",
    "tachyons": "^4.12.0",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "husky": "^5.0.6",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "redux-devtools": "^3.7.0"
  }
}
